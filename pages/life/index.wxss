
.swiper {
  width: 100%;
  height:400rpx
}
.swiper-image {
  width: 100%;
  height:100%
}
.page-section-header {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 40rpx;
}
.page-section-header-text {
  font-size: 32rpx;
  color: red;
}
.page-section {
  display: flex;
  flex-flow: row wrap;
  justify-content: space-between;
  margin-top: 40rpx;
}
.page-cell {
  display: flex;
  flex-direction: column;
  width: 49%;
  margin-top: 10rpx;
  background-color: white;
}

.page-cell-icon {
  width: 100%;
  margin: 20rpx;
  height: 200rpx;
}

.page-cell-name {
  margin: 0rpx auto 10rpx;
  font-size: 16rpx;
}
